{ 
	"env": {
		"browser": true,
    "commonjs": true,
    "es6": true
	},
	"plugins": [
    "jsx-a11y",
    "prettier"
  ],
	"extends": [
		"airbnb-base",
		"react-app",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "prettier/react"
	],
	"rules": {
		"linebreak-style": 0,
		// "semi": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    /* "prettier/prettier": [
      "error", {
        "semi": false
      }
    ] */
	}
}